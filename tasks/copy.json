        "dest": "dist/lib/ace/mode-css.js"
      },
      {
        "src": "node_modules/ace-builds/src-min-noconflict/mode-html.js",
        "dest": "dist/lib/ace/mode-html.js"
      },
      {
        "src": "node_modules/ace-builds/src-min-noconflict/mode-javascript.js",
        "dest": "dist/lib/ace/mode-javascript.js"
      },
      {
        "src": "node_modules/ace-builds/src-min-noconflict/mode-json.js",
