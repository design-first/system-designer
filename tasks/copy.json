      },
      {
        "src": "node_modules/ace-builds/src-min-noconflict/mode-text.js",
        "dest": "dist/lib/ace/mode-text.js"
      },
      {
        "src": "node_modules/ace-builds/src-min-noconflict/worker-css.js",
