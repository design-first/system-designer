      {
        "src": "node_modules/ace-builds/src-min-noconflict/worker-javascript.js",
        "dest": "dist/lib/ace/worker-javascript.js"
      },
      {
        "src": "node_modules/ace-builds/src-min-noconflict/worker-json.js",
        "dest": "dist/lib/ace/worker-json.js"
      }
    ]
